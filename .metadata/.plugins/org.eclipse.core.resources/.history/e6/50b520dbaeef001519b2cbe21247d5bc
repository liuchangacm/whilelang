package lang

/**
 * @author Chang Liu (liuchang@cs.umd.edu)
 */
trait Ast
trait Stat extends Ast
trait Expr extends Ast

case class Skip() extends Stat
case class Assign(v:String, e:Expr) extends Stat
case class Seq(s1:Stat, s2:Stat) extends Stat